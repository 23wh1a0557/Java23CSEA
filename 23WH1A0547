package java547;



public class variabledemo1 {

	String name="akshu";

	public static void main(String[] args) {

		variabledemo1 object1=new variabledemo1();

		variabledemo1 object2=new variabledemo1();

		object1.name="akshaya";

		System.out.println(object1.name);

		System.out.println(object2.name);

	}



}

