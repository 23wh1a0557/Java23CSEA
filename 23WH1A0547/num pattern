import java.util.Scanner;
class HelloWorld {
    public static void main(String[] args) {
        
	    Scanner sc=new Scanner(System.in);
	    System.out.println("enter the size of the rows");
	    int a=sc.nextInt();
	    int i,j,k;
	    for(i=1;i<=a;i++) {
		    for(j=i;j>=1;j--) {
		      System.out.print(j);
		      }
		      for(k=2;k<=i;k++){
		         System.out.print(k);
    	    }System.out.println(" ");
	        }
        }
    }
