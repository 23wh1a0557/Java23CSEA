import java.util.Scanner;
class Main{
    public static void main(String[] args){
        int sum=0,sum1=0;
        Scanner sc=new Scanner(System.in);
        System.out.println("enter n value");
        int n=sc.nextInt();
        for(int i=1;i<=n;i++){
            if(i%2==0){
                sum=sum+i;
            }
            else if(n%2!=0){
                sum1=sum1+i;
            }
        }
        System.out.println(sum);
        System.out.println(sum1);
       
    }
}
